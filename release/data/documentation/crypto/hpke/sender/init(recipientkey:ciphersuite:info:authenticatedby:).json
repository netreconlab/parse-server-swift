    {
      "kind" : "parameters",
      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The recipient’s public key for encrypting the messages.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "recipientKey"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The cipher suite that defines the cryptographic algorithms to use.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "ciphersuite"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Data that the key derivation function uses to compute the symmetric key material. The sender and the recipient need to use the same ",
                  "type" : "text"
                },
                {
                  "code" : "info",
                  "type" : "codeVoice"
                },
                {
                  "text" : " data.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "info"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The sender’s private key for generating the HMAC.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "authenticationKey"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The ",
              "type" : "text"
            },
            {
              "code" : "Sender",
              "type" : "codeVoice"
            },
            {
              "text" : " encrypts messages in authentication mode with a symmetric encryption key.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "Messages also include authentication data so that the recipient can verify the authenticity of the sender’s private key.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The system throws errors from ",
                  "type" : "text"
                },
                {
                  "code" : "CryptoKit\/HPKE\/Errors",
                  "type" : "codeVoice"
                },
                {
                  "text" : " when it encounters them.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
