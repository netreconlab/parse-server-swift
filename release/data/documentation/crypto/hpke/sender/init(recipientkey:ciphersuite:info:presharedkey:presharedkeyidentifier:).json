      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The recipient’s public key for encrypting the messages.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "recipientKey"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The cipher suite that defines the cryptographic algorithms to use.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "ciphersuite"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Data that the key derivation function uses to compute the symmetric key material. The sender and the recipient need to use the same ",
                  "type" : "text"
                },
                {
                  "code" : "info",
                  "type" : "codeVoice"
                },
                {
                  "text" : " data.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "info"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A preshared key (PSK) that the sender and the recipient both hold.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "psk"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "An identifier for the PSK.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "pskID"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The ",
              "type" : "text"
            },
            {
              "code" : "Sender",
              "type" : "codeVoice"
            },
            {
              "text" : " encrypts messages in PSK mode using a symmetric encryption key that the sender and recipient both know in advance, in combination with a key it derives using a key derivation function (KDF) and",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the key schedule data in ",
              "type" : "text"
            },
            {
              "code" : "info",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "The ",
              "type" : "text"
            },
            {
              "code" : "Sender",
              "type" : "codeVoice"
            },
            {
              "text" : " encapsulates the derived key using the recipient’s public key.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "You access the encapsulated key using ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/edu.uky.cs.netreconlab.ParseServerSwift\/documentation\/Crypto\/HPKE\/Sender\/encapsulatedKey",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The system throws errors from ",
                  "type" : "text"
                },
                {
                  "code" : "CryptoKit\/HPKE\/Errors",
                  "type" : "codeVoice"
                },
                {
                  "text" : " when it encounters them.",
