      "content" : [
        {
          "anchor" : "Example",
          "level" : 3,
          "text" : "Example",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A metadata provider may be used to automatically inject metadata such as",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "trace IDs:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "import Tracing \/\/ https:\/\/github.com\/apple\/swift-distributed-tracing",
            "",
            "let metadataProvider = MetadataProvider {",
            "    guard let traceID = Baggage.current?.traceID else { return nil }",
            "    return [\"traceID\": \"\\(traceID)\"]",
            "}",
            "let logger = Logger(label: \"example\", metadataProvider: metadataProvider)",
            "var baggage = Baggage.topLevel",
            "baggage.traceID = 42",
            "Baggage.withValue(baggage) {",
            "    logger.info(\"hello\") \/\/ automatically includes [\"traceID\": \"42\"] metadata",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "We recommend referring to ",
              "type" : "text"
            },
            {
              "identifier" : "https:\/\/github.com\/apple\/swift-distributed-tracing",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "for metadata providers which make use of its tracing and metadata propagation infrastructure. It is however",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "possible to make use of metadata providers independently of tracing and instruments provided by that library,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "if necessary.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    }
